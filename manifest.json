{
  "name": "langswap",
  "version": "1.0",
  "manifest_version": 3,
    "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": 
      [
          {
              "matches": ["*://*/*"],
              "all_frames": true,
              "js": ["content_script.js"],
              "run_at": "document_end"
          }
      ]
}
